<%	
	const tocContent = toc( trim(post.content) );
	if(theme.post_toc.enable && tocContent) { 
%>
	<% if(theme.post_toc.katelog) { %>
		<div class="layout-toc">
			<div id="layout-toc">
				<div class="k-catelog-list" id="catelog-list" data-title="文章目录"></div>
			</div>
		</div>

		
		<%- js('plugin/toc/katelog.min.js') %>

	<% } else {%>
		<%# TODO: You need other toc plugin? %>
	<% } %>	
<% 
	} 
%>	